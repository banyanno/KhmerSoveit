                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ComBindRefferal, COUNT(Male) AS M, COUNT(Female) AS F, '0-4' AS Age
FROM V_Referal_New_Old
WHERE (Age BETWEEN 0 AND 4) AND (CAST(CONVERT(VARCHAR(10), CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND 
          CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY ComBindRefferal
UNION
SELECT ComBindRefferal, COUNT(Male) AS M, COUNT(Female) AS F, '5-14' AS Age
FROM V_Referal_New_Old AS V_Referal_3
WHERE (Age BETWEEN 5 AND 14) AND (CAST(CONVERT(VARCHAR(10), CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND 
          CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY ComBindRefferal
UNION
SELECT ComBindRefferal, COUNT(Male) AS M, COUNT(Female) AS F, '15-49' AS Age
FROM V_Referal_New_Old AS V_Referal_2
WHERE (Age BETWEEN 15 AND 49) AND (CAST(CONVERT(VARCHAR(10), CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND 
          CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY ComBindRefferal
UNION
SELECT ComBindRefferal, COUNT(Male) AS M, COUNT(Female) AS F, '&gt;=50' AS Age
FROM V_Referal_New_Old AS V_Referal_1
WHERE (Age &gt;= 50) AND (CAST(CONVERT(VARCHAR(10), CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), 
          CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY ComBindRefferal</CommandText>
